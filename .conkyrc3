# Specific
gap_x 10
gap_y 30
update_interval 20
minimum_size 170 0
maximum_width 360


# General wide
use_xft yes
xftfont caviar dreams:size=10
xftalpha 0.8
uppercase no
background yes
cpu_avg_samples 2
net_avg_samples 2
temperature_unit celsius
no_buffers yes
text_buffer_size 256
default_color FFFFFF
color1 3f43ff  #Info  letter color
color2 000000  #Label letter color
color3 000000  #Title letter color
color4 FFFFFF  #None
color5 ebedf1  #separator color

own_window_transparent yes 
own_window_type normal
own_window_argb_visual yes 
own_window_class override
own_window yes
own_window_argb_value 90 #value glass 0-250 full #own_window_argb_visual yes
own_window_hints undecorated,below,sticky,skip_taskbar,skip_pager
double_buffer yes
total_run_times 0
 
# Minimum size of text area

draw_shades no
draw_outline no
draw_borders no
#draw_graph_borders yes
stippled_borders 0
#border_margin 5
border_width 3

#Default colors and also border colors
default_color 05FBE8
#default_shade_color black
#default_outline_color grey
own_window_colour black

#Text alignment, other possible values are commented
#alignment top_left
alignment top_right
#alignment bottom_left
#alignment bottom_right

 
override_utf8_locale yes
use_spacer none









